{
  "name": "juno-nextjs-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier:write": "prettier --write --ignore-unknown .",
    "test": "jest --coverage --passWithNoTests",
    "test:w": "jest --watch",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "eslint",
      "prettier --write"
    ],
    "src/common/locales/*.json": [
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.12.2",
    "@emotion/cache": "^11.13.1",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@growthbook/growthbook-react": "^1.4.1",
    "@mui/material": "^7.0.0",
    "@reduxjs/toolkit": "^2.3.0",
    "axios": "^1.7.7",
    "classnames": "^2.5.1",
    "i18next": "^23.16.4",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "next": "^15.0.1",
    "next-intl": "^3.26.5",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.1.0",
    "react-redux": "^9.1.2",
    "sass": "^1.86.0",
    "uuid": "^11.0.0",
    "what-is-my-ip-address": "^1.0.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.8.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.13.0",
    "eslint-config-next": "^15.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-testing-library": "^6.4.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "serve": "^14.2.4",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  }
}
